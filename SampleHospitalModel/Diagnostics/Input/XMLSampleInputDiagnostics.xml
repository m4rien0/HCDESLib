  </StaffHandling>

  <ConsiderAdmissionForPath>true</ConsiderAdmissionForPath>
  <UseImmediateBookingModel>true</UseImmediateBookingModel>
  
  <Paths>

    <Path ID="0">
      <PathSteps>
        <Step SingleAction ="Lab"></Step>
        <Step SingleAction ="XRay"></Step>
        <Step SingleAction ="Return"></Step>
